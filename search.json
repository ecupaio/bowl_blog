---
layout: null
---
[
  {% for post in site.recipes %}
    {
      "title"    : "{{ post.title | escape }}",
      "ingredients" : "{{ post.ingredients }}",
      "instructions": "{{ post.content }}",
      "notes": "{{ post.notes }}",
      "url"      : "{{ post.url }}",
      "date"     : "{{ post.date }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
